main-color = #1abc9c

[ng\:cloak], [ng-cloak], [data-ng-cloak], [x-ng-cloak], .ng-cloak, .x-ng-cloak
	display none !important

*::-webkit-scrollbar
	width 1px
	background-color white
*::-webkit-scrollbar-track-piece
	background-color #ededed
	border 0px
*::-webkit-scrollbar-thumb 
	background-color #777


*::-webkit-input-placeholder
	font-weight 300
*:-moz-placeholder
	font-weight 300
*::-moz-placeholder
	font-weight 300
*:-ms-input-placeholder
	font-weight 300


body,html
	height 100%
body
	padding-top 1px
	margin-top -1px
h1,h3
	text-align center
h3
	font-weight 300
h4
	font-weight 200

#map
	position absolute
	top 0px
	left 0px
	width 100%
	height 100%
	// background-color #eee
	z-index 0
#panel
	position relative
	height 100%
	z-index 1
	background-color rgba(200,200,250,0.7)
	overflow-x hidden
#welcome_panel
	padding-top 1px
	margin-top -1px
	// background-color rgba(250,200,200,0.7)
	// background-color #333344
	background-color #FFFFFF

.ar-badges
	margin 0 auto 30px auto
	overflow auto
	max-width 1400px

p.ar-center
	text-align center

.ar-badge
	background-color transparent
	background-image url(../img/email.svg)
	background-position center center
	background-size contain
	background-repeat no-repeat
	width 95%
	height 170px 	

#search_panel
	z-index 3
	background-color rgba(250,250,250,0.95)

.ar-panel
	height 100%
	width 100%
	position absolute
	overflow-y scroll
	overflow-x hidden
	padding-top 1px
	margin-top -1px
	-webkit-transition margin .25s ease
	-moz-transition margin .25s ease
	-ms-transition margin .25s ease
	-o-transition margin .25s ease
	transition margin .25s ease

.ar-wrapper
	max-width 800px
	margin 0 auto
	padding 17px
.ar-wrapper.w8
	max-width 650px
	
.ar-welcome-options
	text-align center
.ar-welcome-options button
	display inline-block
	max-width 200px

ul.ar-travels
	list-style none
ul.ar-travels li
	display block
	min-height 100px
	text-align justify
	cursor pointer
	margin 30px 0


ul.ar-travels li div
	display inline-block

.ar-travel-type
	width 40px
	height 30px
	margin-right 10px
	font-family 'Glyphicons Halflings'
	font-style normal
	font-weight 400
	background-size contain
	background-repeat no-repeat
	background-position center center
	opacity 0.5
.ar-transport-air
	background-image url(../img/plane.svg)
.ar-transport-land
	background-image url(../img/bus.svg)
.ar-transport-sea
	background-image url(../img/boat.svg)
.ar-transport-unk
	background-image url(../img/travel.svg)
	
.ar-travel-cost
	font-size 1.45em
.ar-currency
	font-size 12px
	color #999
	margin-left 7px
.ar-travel-cost
	min-width 150px
	text-align right
.ar-time
	margin-left 10px
	width 80px
.ar-star-err
	font-size 27px
	width 15px
.ar-name
	font-style italic
	font-size 1.2em
	color #777
	text-transform capitalize
.ar-from-to
	display block !important
	width 100%
	text-align center
	color #999
	font-size 0.9em
	margin-top -7px

.ar-from-to span
	border-bottom 2px solid #999
	width 50px
	height 12px
	vertical-align top
	display inline-block

.ar-view
	background-color main-color
	border-radius 50%
	width 50px
	height 50px
	text-align center
	margin-right 10px
	margin-top 10px
	background-image url(../img/inspect.svg) 
	background-repeat no-repeat 
	background-position center center
	background-size 65%
	display block
	float right
	opacity 0
	-webkit-transition: all 0.1s ease-in-out;
	-moz-transition: all 0.1s ease-in-out;
	-ms-transition: all 0.1s ease-in-out;
	-o-transition: all 0.1s ease-in-out;
	transition: all 0.1s ease-in-out;

ul.ar-travels li:hover .ar-view
	opacity 1

.ar-text-right
	text-align right

.ar-max500
	max-width 500px
	margin 0 auto

.btn-primary
	background-color transparent
	border 1px solid main-color
	color main-color

#welcome_panel.out
	margin-left 100%
#welcome_panel.almost
	margin-left 95%


#search_panel.out
	margin-left -100% 
#search_panel.in
	margin-left 0px



#travel_panel
	background-color #FFFFFF
	width 70%
	right 0px
	.ar-travel-type
		width 40px
		height 40px
		margin-left 50px
		float left
	.ar-travel-cost
		font-size 1.6em
		display inline-block
	.ar-currency
		display inline-block
		font-size 1.1em 
		height 100%
		text-align left
		vertical-align 0px
	.ar-from-to
		font-size 1em
		overflow auto
		text-align left
	.ar-from-to span
		height 18px
		width 35%
	.ar-date
		font-size 1.1em
		text-align left
	.ar-date span
		color #777
		font-size 0.8em
	.ar-from-to
		margin 10px auto

#travel_panel //continues
	.ar-date span.ar-frmto
		border-bottom 1px solid #999
		width 100px
		display inline-block
	.ar-person
		font-size 1.8em
		text-align center
		width 100%
		margin-top 20px
	.ar-person span
		width 100%
		display block
		font-size 0.4em
		text-transform capitalize
	p
		margin-left 20px
	h7
		width 100%
		display block
		font-weight bold
	.ar-name
		display inline-block
		vertical-align top
	.ar-stuff-maybe-someone-will-ever-read h6
		text-indent 0px
	.ar-stuff-maybe-someone-will-ever-read
		margin-top 40px

.ar-person-photo
	height 60px
	width 60px
	background-color #eee
	display inline-block
	border-radius 50%
	margin-right 20px
	background-image url(../img/user2.svg)
	background-repeat no-repeat
	background-position-x 49%
	background-position-y 13px
	background-size 105%

.ar-hotel,.ar-airline
	background-repeat no-repeat
	background-position left center
	background-position-x 0px
	background-size contain
	display block
	text-indent 40px

.ar-gdetails
	height 70px
	max-width 600px
	height 40px
	margin 0 auto
	margin-top 60px

.ar-hotel
	background-image url(../img/hotel.svg)
	width 250px
	float left
.ar-airline
	background-image url(../img/ticket.svg)
	width 200px
	float right
.ar-stuff p
	margin 0 auto
	display block
	font-size 0.75em
.ar-stuff p.ar-pbigger
	font-size 1.0em
	font-weight bold


#pdf
	background-image url(../img/pdf.svg)
#pdf:hover .ar-borderer
	border 1px solid #E41D24
#twitter
	background-image url(../img/twitter.svg)
#twitter:hover .ar-borderer
	border 1px solid #96CBE4
#facebook
	background-image url(../img/facebook.svg)
#facebook:hover .ar-borderer
	border 1px solid #3C5A98
#email
	background-image url(../img/email.svg)
#email:hover .ar-borderer
	border 1px solid #999999
.ar-clear
	clear both
a.ar-actions, a.ar-actions:hover
	color inherit
.ar-actions
	background-position center 10px
	background-repeat no-repeat
	background-size 20%
	display block
	margin-top 80px	
	text-align center
	cursor pointer
	font-size 0.9em

.ar-actions .ar-borderer
	padding-top 60px
	border 1px solid #fff
	border-radius 5px
	max-width 150px
	margin 0 auto
	height 100%

.ar-institution
	max-width 600px
	margin 0 auto

.ar-spacer
	height 35px
	width 100%
	clear both

.ar-search-bar
	max-width 400px
	margin 20px auto

input.ar-search
	background-color transparent
	border 1px solid #999999
	border-radius 27px
	text-indent 40px
	font-size 1.5em
	width 100%
	outline none
	background-image url(../img/search.svg)
	background-position 10px center
	background-repeat no-repeat
	background-size 5%
input.ar-search:focus
	border 1px solid main-color
	background-image url(../img/search2.svg)
	

.backbtn
	width 65px
	height 65px
	border 1px solid #999
	border-radius 50%
	background-image url(../img/arrow-right.svg)
	background-position center center
	background-repeat no-repeat
	background-size 40%
	position fixed
	top 20px
	right 1%
	cursor pointer
	display none
.ar-panel.in .backbtn
	display block

#debug.wtf
	position absolute
	z-index 9
	background #FF0000
	height 50px
	width 100%

.loader
	width 200px
	height 40px
	margin 0 auto
	opacity 0.0
	-webkit-transition opacity .25s ease-out
	-moz-transition opacity .25s ease-out
	-ms-transition opacity .25s ease-out
	-o-transition opacity .25s ease-out
	transition opacity .25s ease-out

.loader.loading
	opacity 1.0

.loader svg
	fill main-color
	width 100%
	height 100%

ul.ar-autocomplete-list
	background-color rgba(250,250,250,0.92)
	box-shadow 0px 0px 3px #DDD
	display block
	max-width 350px
	margin-left 25px
	list-style none
	padding 0px
	position absolute
	width 100%
	z-index 2
ul.ar-autocomplete-list li
	border-bottom 1px solid #DDDDDD
	padding-top 10px
	padding-bottom 10px
	display block
	text-indent 20px
	text-transform capitalize
	text-overflow ellipsis
	white-space nowrap
	overflow hidden

ul.ar-autocomplete-list li.active
	background-color main-color
	color #ffffff
	cursor pointer

.fui-money
	content "$"
	font-weight bold

.ar-btn-wrapper
	text-align center
	display block
	width 100%
	height 50px
.btn-group .btn
	border-right 0px	
.btn-group .btn:last-child
	border-right 1px solid main-color

ul.ar-options-list
	max-width 570px
	margin 0 auto
	list-style none
	padding 0px

ul.ar-autocomplete-list.ar-out
	display none

ul.ar-options-list .ar-money
	max-width 300px


ul.ar-options-list li
	display block
	min-height 90px
	text-align justify
	position: relative;
	cursor pointer
ul.ar-options-list li .ar-view
	position absolute
	right -90px
	top 10px
	width 40px
	height 40px
ul.ar-options-list li:hover .ar-view
	opacity 1.0
	

ul.ar-options-list li .ar-option-upper
	width 100%
	min-height 45px	
ul.ar-options-list li .ar-option-middler
	width 100%
	overflow auto
ul.ar-options-list li .ar-option-bottom
	margin-bottom 20px

ul.ar-options-list li .ar-money
	font-size 1.5em
	float left
	max-height 45px
ul.ar-options-list li .ar-transport
	max-width 150px
	font-size 14px
	float right
ul.ar-options-list li .ar-person-photo
	height 25px
	width 25px
	background-position-x 49%
	background-position-y 4px
	background-size 105%
	float left
ul.ar-options-list li .ar-name
	float left
ul.ar-options-list li .ar-date
	float right
ul.ar-options-list li .ar-money span
	font-size 0.5em
	color #999999
	valign bottom

ul.ar-options-list li .ar-transport
	opacity 0.7
	text-indent 30px
	background-size 12%
	background-repeat no-repeat
	background-position left center
ul.ar-options-list li .ar-date
	opacity 0.7
	text-indent 30px
	background-size 12%
	background-repeat no-repeat
	background-position left center	
	background-image url(../img/calendar.svg)

ul.ar-options-list .ar-from-to
	position relative
ul.ar-options-list .ar-from-to div
	display block
	min-width 160px
ul.ar-options-list .ar-from-to div.ar-to
	float right
	background-color transparent
	text-align right
ul.ar-options-list .ar-from-to div.ar-from
	float left
	background-color transparent
	text-align left


//login
.login-block
	max-width 400px	
	margin 5% auto 0px auto
.cui-login:before
	font-family 'Flat-UI-Icons'
	content "\e009   "
#login_panel,
#user_panel,
#loading_panel
	background-color #fff
#login_panel,
#user_panel
	display none
.ar-adm .ar-panel
	z-index 0
.ar-panel.in 
	z-index 3


#loading_panel
	padding-top 100px
#loading_panel .loader
	width 80%
	height 100px
	margin-top 100px


.ar-panel.aractive,
#login_panel.ar-panel.aractive,
#user_panel.ar-panel.aractive
	z-index 3
	display block
	

.ar-advice
	position absolute
	width 300px
	background-color #F39C12
	color #FFFFFF
	padding 10px
	left 50%
	margin-left -150px
	text-align center
	border-radius 0px 0px 4px 4px
	-webkit-transition: margin .45s ease-out;
	   -moz-transition: margin .45s ease-out;
	    -ms-transition: margin .45s ease-out;
	     -o-transition: margin .45s ease-out;
	        transition: margin .45s ease-out;
	margin-top -60px

.form-group
	position relative
span.input-group-btn
	display block
	position absolute
	bottom 0px
	right 39px
.ar-advice.ar-in
	margin-top 0px
.ar-form
	max-width 600px
	margin 0 auto
	padding 24px 23px 20px
	background-color rgba(255,255,255,0.90)
.ar-form input,
.ar-form select,
.ar-form textarea
	background-color transparent
select
	width 100%
	border 2px solid #bdc3c7
	border-radius 6px
	padding 8px 12px
.ar-form-section label
	display block
	width 100%
	border-bottom 1px solid #999999
	font-size 1.1em	
	font-weight bold
.ar-sponsors
	width 100%
	min-height 120px
	padding-top 40px
	padding-bottom 40px
	overflow auto
.ar-sponsor
	height 90px
	background-color royalblue
.ar-logo
	margin 0 auto
	margin-top 50px
	height 150px
	width 200px
	background-color main-color
.login-form
	background-color transparent
.login-form .login-field
	border 1px solid #ccc
p h6
	text-align center

.ar-subcription input
	border 1px solid #bdc3c7
	border-radius 6px 6px 0px 0px
	border-bottom 0px
	text-align center
	-webkit-transition all .25s ease-out
	-moz-transition all .25s ease-out
	-ms-transition all .25s ease-out
	-o-transition all .25s ease-out
	transition all .25s ease-out
.ar-subcription button
	border-radius  0px 0px 6px 6px
form.ar-subcription
	max-width 500px
	input.listo
		height 0px
		padding 0px
	label.listo
		display none
	button.listo
		border-radius 6px
	button span 
		display none
	button.listo span 
		display block

.ar-top-info
	overflow auto

.ar-long-text-line
	overflow hidden
	text-overflow ellipsis
	white-space nowrap
	font-size 0.7em
	color #999

.ar-travel-transport
	padding-top 5px
	width 390px
	float left
.ar-travel-title
	font-size 1.4em
	max-width 380px



//everything
@media(min-width:1400px)
	.backbtn
		right 50px
@media(min-width:1200px)
	#travel_panel .ar-name
		min-width 450px

@media(min-width:1000px) and (max-width:1400px)
	.ar-text-right
		margin-right 70px

//login
@media(min-width:1000px)
	.ar-loginbigblock
		max-width 600px
		margin 0 auto
		overflow auto
		.ar-logo
			float left
			margin-top 90px
		.login-block
			float right
			width 390px
@media(min-height:600px)
	.ar-loginbigblock
		margin-top 150px
		margin-top 10vh		

//travel panel
@media(min-width:726px) and (max-width:1200px)
	#travel_panel .ar-wrapper
		padding-left 20px
	#travel_panel .ar-name
		width 100%

//search panel
@media(max-width:725px)
	ul.ar-options-list li .ar-name,
	ul.ar-travels 
		li div.ar-name
			width 100%
		li div.ar-from-to
			text-align left
		li div.ar-view
			position absolute
			right 20px
	ul.ar-options-list li
		.ar-name
			width 100%
			margin-left 32px
			margin-top -25px
			float none
		.ar-person-photo
			margin 0px
		.ar-date
			width 100%
			background-size 21px
		.ar-option-upper
			min-height 30px

//travel panel
@media(max-width:1000px)
	.col-xs-8.ar-text-right
		float right

	#travel_panel	
		width 100%
		.ar-name
			width 100%
		h6,h7
			text-indent 0px
		p
			text-indent 5px
			margin-left 10px
		.ar-travel-type
			margin-left 0px
			
	#travel_panel.ar-panel.in .backbtn,
	#search_panel.ar-panel.in .backbtn
		position static
		margin 0 auto 50px auto		

	#search_panel.ar-panel .backbtn
		right 5%
		

@media(max-width:800px)
	#travel_panel .ar-travel-type
		margin-left 0px
		padding-left 0px
		width 30px
		height 30px
	#travel_panel .ar-date
		float left

@media(max-width:1000px)
	#travel_panel .ar-from-to span
		width 5%

//everything
@media(max-width:450px)
	h1,.h1
		font-size 2.0em
	#welcome_panel
		ul.ar-travels
			padding 0px
		ul.ar-travels li div
			text-align left
		.ar-travel-cost
			width auto
			min-width 50px
		.ar-from-to span
			width 10px
		ul.ar-travels li:hover .ar-view
			opacity 0.0

.ar-credits
	max-width 1200px
	margin 0 auto
	font-size 0.8em
	text-align right
	overflow auto
	min-height 40px
	div
		display block
	div.ar-left
		margin 0 10px
		float left
	div.ar-right
		float right
		margin-right 10px
